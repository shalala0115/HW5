{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://JW-sudo.github.io/my_design",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "css": "^2.2.4",
    "fg-loadcss": "^2.1.0",
    "git": "^0.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.2.0",
    "react-syntax-highlighter": "^11.0.2",
    "react-vis": "^1.11.7",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build-js",
    "css": "stylus -w src/stylus/index.styl -o src",
    "stylus": "stylus -w src/stylus/index.styl -o src",
    "build-css": "stylus -c src/stylus/ --out src",
    "watch-css": "yarn run build-css && stylus -c -w src/stylus/ --out src",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "install": "^0.13.0",
    "npm": "^6.11.3",
    "npm-run-all": "^4.1.5"
  }
}
